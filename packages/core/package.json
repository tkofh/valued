{
  "name": "valued",
  "version": "0.1.0",
  "description": "parse strings as css values",
  "author": "tim morris <timthewebguy@gmail.com>",
  "homepage": "https://github.com/tkofh/css-value-parser#readme",
  "license": "MIT",
  "type": "module",
  "main": "dist/parse.cjs",
  "module": "dist/parse.mjs",
  "types": "dist/parse.d.ts",
  "exports": {
    ".": {
      "import": "./dist/parse.mjs",
      "require": "./dist/parse.cjs",
      "types": "./dist/parse.d.ts"
    },
    "./data": {
      "import": "./dist/data.mjs",
      "require": "./dist/data.cjs",
      "types": "./dist/data.d.ts"
    },
    "./combinators": {
      "import": "./dist/combinators.mjs",
      "require": "./dist/combinators.cjs",
      "types": "./dist/combinators.d.ts"
    },
    "./multipliers": {
      "import": "./dist/multipliers.mjs",
      "require": "./dist/multipliers.cjs",
      "types": "./dist/multipliers.d.ts"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest run"
  },
  "bugs": {
    "url": "https://github.com/tkofh/css-value-parser/issues"
  },
  "devDependencies": {
    "@repo/tsconfig": "workspace:^",
    "@types/node": "^20.14.2",
    "@vitest/coverage-v8": "^1.6.0",
    "tsx": "^4.15.5",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "colorjs.io": "^0.5.0"
  }
}
